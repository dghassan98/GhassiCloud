# Server Configuration
PORT=3001
NODE_ENV=development

# Database
DATABASE_PATH=./data/ghassicloud.db

# JWT Secret (change in production!)
JWT_SECRET=your-secret-key-change-in-production

# Default Admin User (only used on first run)
DEFAULT_ADMIN_USER=admin
DEFAULT_ADMIN_PASS=admin

# Keycloak SSO Configuration (PKCE flow - no client secret needed)
KEYCLOAK_URL=https://auth.ghassandarwish.com
KEYCLOAK_REALM=master
KEYCLOAK_CLIENT_ID=ghassicloud
